<?xml version="1.0" encoding="utf-8"?>
<isys_export>
	<head>
		<datetime>@@CURRENT_DATE@@</datetime>
		<mandator language="" id="1">@@COMPANY_NAME@@</mandator>
		<type>openQRM_auto_documentation</type>
		<version>0.9.9-3</version>
	</head>
	<objects count="1">
		<object>
			<id>1</id>
			<title>@@SYSTEM_NAME@@ (@@SYSTEM_MAC_ADDRESS@@)</title>
			<sysid>SYSID_@@SYSTEM_ID@@</sysid>
			<created by="admin">@@CURRENT_DATE@@</created>
			<type id="5" const="C__OBJTYPE__SERVER" title_lang="LC__CMDB__OBJTYPE__SERVER">Server</type>
			<status>2</status>

			<data>
				<category title="Allgemein" const="C__CATG__GLOBAL" category_type="0">
					<cat_data data_id="14">
						<category id="1" const="C__GLOBAL_CATEGORY__OTHER" title_lang="LC__UNIVERSAL__OTHER">Andere</category>
						<description><![CDATA[@@SYSTEM_DESCRIPTION@@]]></description>
					</cat_data>
				</category>

				<category title="Hostadresse" const="C__CATG__IP" category_type="0">
					<cat_data data_id="1">
						<hostname>@@SYSTEM_MAC_ADDRESS@@ (@@SYSTEM_NAME@@)</hostname>
						<dhcp>0</dhcp>
						<address>@@SYSTEM_IP_ADDRESS@@</address>
						<mask>@@SYSTEM_SUBNET_MASK@@</mask>
						<gateway>@@SYSTEM_DEFAULT_GATEWAY@@</gateway>
						<primary>1</primary>
						<active>1</active>
						<assignment id="2" const="C__CATP__IP__ASSIGN__STATIC" title_lang="LC__CATP__IP__ASSIGN__STATIC">Statisch</assignment>
						<net_type id="1" const="C__CATS_NET_TYPE__IPV4" title_lang="IPv4 (Internet Protocol v4)">IPv4 (Internet Protocol v4)</net_type>
					</cat_data>
				</category>

			</data>
		</object>
	</objects>
</isys_export>
